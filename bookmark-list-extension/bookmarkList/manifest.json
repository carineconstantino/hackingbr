{
    "manifest_version": 2,
    "name": "Bookmark List",
    "version": "1.0",
    "description": "Make a list from yours favorities links easily accessible",
    "permissions": ["bookmarks"],

    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "chrome_url_overrides": {  "newtab": "popup.html"}
  }
  

